{
      "manifest_version": 3,
      "name": "Download Header Inspector",
      "version": "1.0",
      "permissions": ["declarativeNetRequest", "downloads", "notifications"],
      "host_permissions": ["<all_urls>"],
      "background": {
        "service_worker": "background.js"
      },
      "declarative_net_request": {
        "rule_resources": [
          {
            "id": "ruleset",
            "enabled": true,
            "path": "rules.json"
          }
        ]
      }
    }
    