{
  "manifest_version": 3,
  "name": "AI file downloader",
  "version": "1.0",
  "description": "Blocks downloads with MIME type application/pdf.",
  "permissions": [
    "downloads","windows","activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ], "action": {
    "default_popup": "popup.html"}

}
