{
  "manifest_version": 3,
  "name": "file downloader",
  "version": "1.0",
  "description": "Blocks downloads with MIME type application/pdf.",
  "permissions": [
    "downloads","windows","activeTab","system.display"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]

}
