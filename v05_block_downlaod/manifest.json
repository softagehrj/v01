{
      "manifest_version": 3,
      "name": "PDF Download Blocker",
      "version": "1.0",
      "description": "Blocks or intercepts PDF downloads.",
      "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestFeedback",
        "activeTab"
      ],"action":{
            "default_popup": "popup.html"  
  },
      "host_permissions": [
        "<all_urls>"
      ],
      "declarative_net_request": {
        "rule_resources": [{
          "id": "ruleset_1",
          "enabled": true,
          "path": "rules.json"
        }]
      }
    }
    